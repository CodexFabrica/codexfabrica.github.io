---
layout: null
---
[
    {% for product in site.products %
    }
  {
        "title": "{{ product.title | xml_escape }}",
        "url": "{{ site.url }}{{ site.baseurl }}{{ product.url }}",
        "description": "{{ product.description | xml_escape }}",
        "tagline": "{{ product.tagline | xml_escape }}",
        "product_logo": "{{ site.url }}{{ site.baseurl }}{{ product.product_logo }}"
    }{% unless forloop.last %
    },
    {% endunless %
    }
  {% endfor %
    }
]